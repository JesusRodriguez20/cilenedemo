<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="msp.cilenedemo">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ActivityInformacionGeneral"
            android:parentActivityName=".activities.ActivityMain">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ActivityMain">
            </meta-data>
        </activity>
        <activity
            android:name=".activities.ActivityMisCupones"
            android:parentActivityName=".activities.ActivityMain">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ActivityMain">
            </meta-data>
        </activity>
        <activity
            android:name=".activities.ActivityAboutMsp"
            android:parentActivityName=".activities.ActivityMain">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".activities.ActivityMain">
            </meta-data>
        </activity>
    </application>
</manifest>
